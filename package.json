{
	"name": "@bybas/latte-ui",
	"version": "1.5.0-beta.3",
	"private": true,
	"scripts": {
		"build": "yarn workspace @bybas/latte-ui run build",
		"serve": "yarn workspace @bybas/latte-ui run serve",
		"docs:build": "yarn workspace latte-ui-docs run build",
		"docs:serve": "yarn workspace latte-ui-docs run serve",
		"publish": "yarn workspace @bybas/latte-ui publish --access public",
		"publish:beta": "yarn workspace @bybas/latte-ui publish --access public --tag beta"
	},
	"workspaces": [
		"packages/*"
	],
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not ie <= 8"
	]
}
