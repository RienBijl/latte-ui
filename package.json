		"@babel/polyfill": "^7.0.0",
		"@babel/preset-env": "^7.1.6",
		"autoprefixer": "^9.3.1",
		"babel-preset-es2015": "^6.24.1",
		"chokidar": "^2.0.4",
		"ncp": "^2.0.0",
